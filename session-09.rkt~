#lang racket


;------- Session 09 -------;

;------- opening exercise -------;
(define any-bigger-than?
  (lambda (n lon)
    (> (apply max '(26 37 41 25 12)) 40)
    ))

;------- recursive function for power -------;
(define power
  (lambda (x n)
    (if (zero? n)
        1
        (* x (power x (sub1 n))))))

;------- recursive function for power -------;
